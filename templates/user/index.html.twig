{% extends 'base.html.twig' %}
{% block title %}{{ title }}{% endblock %}
{% block body %}
    <h1>
        {{ title }}
    </h1>

<a href="{{ path('admin_user_create') }}">Создать пользователя</a><br>

    <h2>Список пользователей</h2>
                <ul>
                    {% for user in users %}           <!-- перебор и вывод всех пользователей -->
                        <li>{{ user.username }}, {{ user.roles | json_encode }}</li>
                    {% endfor %}
                </ul>


{% endblock %}
